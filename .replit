
run = "npm run start"
hidden = [".config", "package-lock.json"]
modules = ["nodejs-20"]

[env]
PORT = "3000"
NODE_ENV = "production"

[nix]
channel = "stable-22_11"

[deployment]
run = ["sh", "-c", "npm run start"]
deploymentTarget = "cloudrun"
ignorePorts = false

[[ports]]
localPort = 3000
externalPort = 80
